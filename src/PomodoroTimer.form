<?xml version="1.0" encoding="UTF-8"?>
<form xmlns="http://www.intellij.com/uidesigner/form/" version="1" bind-to-class="PomodoroTimer">
  <grid id="27dc6" binding="panelMain" layout-manager="BorderLayout" hgap="0" vgap="0">
    <constraints>
      <xy x="20" y="20" width="500" height="400"/>
    </constraints>
    <properties/>
    <border type="none"/>
    <children>
      <toolbar id="7c1fc" binding="toolBar">
        <constraints border-constraint="North"/>
        <properties/>
        <border type="none"/>
        <children>
          <component id="a5f1c" class="javax.swing.JButton" binding="buttonAddTodo">
            <constraints/>
            <properties>
              <text value="Add To-Do"/>
            </properties>
          </component>
          <component id="66522" class="javax.swing.JButton" binding="buttonFinishTodo">
            <constraints/>
            <properties>
              <text value="Finish To-Do"/>
            </properties>
          </component>
        </children>
      </toolbar>
      <grid id="f91c8" binding="panelTimer" layout-manager="GridBagLayout">
        <constraints border-constraint="Center"/>
        <properties/>
        <border type="none"/>
        <children>
          <component id="b1f93" class="javax.swing.JLabel" binding="labelCurrentTimer">
            <constraints>
              <grid row="0" column="0" row-span="1" col-span="1" vsize-policy="0" hsize-policy="0" anchor="0" fill="0" indent="0" use-parent-layout="false"/>
              <gridbag weightx="0.0" weighty="0.0"/>
            </constraints>
            <properties>
              <font name="Segoe UI" size="36"/>
              <text value="Pomodoro"/>
            </properties>
          </component>
          <component id="b712f" class="javax.swing.JButton" binding="buttonStartTimer">
            <constraints>
              <grid row="2" column="0" row-span="1" col-span="1" vsize-policy="0" hsize-policy="3" anchor="0" fill="1" indent="0" use-parent-layout="false"/>
              <gridbag weightx="0.0" weighty="0.0"/>
            </constraints>
            <properties>
              <text value="Start Timer"/>
            </properties>
          </component>
          <component id="f1764" class="javax.swing.JLabel" binding="labelTimer">
            <constraints>
              <grid row="1" column="0" row-span="1" col-span="1" vsize-policy="0" hsize-policy="0" anchor="0" fill="0" indent="0" use-parent-layout="false"/>
              <gridbag weightx="0.0" weighty="0.0"/>
            </constraints>
            <properties>
              <font name="Segoe UI" size="26"/>
              <text value="25:00"/>
            </properties>
          </component>
        </children>
      </grid>
      <scrollpane id="b5a7c" binding="scrollPane">
        <constraints border-constraint="South"/>
        <properties/>
        <border type="none"/>
        <children>
          <component id="35a0d" class="javax.swing.JList" binding="listTodo">
            <constraints/>
            <properties/>
          </component>
        </children>
      </scrollpane>
    </children>
  </grid>
</form>
